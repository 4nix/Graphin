{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "allowJs": false,
    "target": "es2015",
    "moduleResolution": "node",
    "jsx": "react",
    "resolveJsonModule": true,
    "strict": true,
    "noImplicitAny": true,
    "lib": ["dom", "esnext"],
    "declaration": true,
    "esModuleInterop": true,
    "strictNullChecks": false,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "sourceMap": true,
    "skipLibCheck": true,
    "isolatedModules": false,
    "downlevelIteration": true
  },
  "include": ["src"]
}
