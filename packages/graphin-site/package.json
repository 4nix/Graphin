{
  "name": "graphin-site",
  "version": "1.1.0-beta.1",
  "description": "antv graphin site",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "npm run site:develop",
    "site:develop": "gatsby develop --open",
    "site:build": "npm run site:clean && gatsby build --prefix-paths",
    "site:clean": "gatsby clean",
    "site:deploy": "npm run site:build && gh-pages -d public"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/antvis/graphin"
  },
  "devDependencies": {
    "@antv/gatsby-theme-antv": "^0.10.62",
    "@antv/graphin": "^1.0.2",
    "gatsby": "^2.17.7",
    "gh-pages": "^2.1.1"
  },
  "dependencies": {
    "@antv/graphin": "*",
    "@antv/graphin-components": "*",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-i18next": "^11.0.1"
  }
}
